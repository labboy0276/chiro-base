{
    "name": "chiro_base",
    "description": "Chiro Distribution",
    "type": "drupal-profile",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "John Ouellet",
            "email": "labboy0276@yahoo.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "git",
            "url": "https://github.com/labboy0276/chiro_bs_base"
        },
        {
            "type": "git",
            "url": "https://github.com/labboy0276/bootstrap"
        }
    ],
    "extra": {
        "installer-paths": {
            "libraries/{$name}": ["type:drupal-library"],
            "modules/contrib/{$name}": ["type:drupal-module"],
            "themes/contrib/{$name}": ["type:drupal-theme"],
            "modules/custom/{$name}": ["type:drupal-custom-module"],
            "themes/custom/{$name}": ["type:drupal-custom-theme"]
        }
    },
    "require": {
        "composer/installers": "^1.2",
        "drupal/config_readonly": "^1.0@beta",
        "drupal/config_split": "^1.2",
        "drupal/adminimal_theme": "^1.3",
        "drupal/paragraphs": "^1.2",
        "drupal/bootstrap": "^3.6",
        "labboy0276/chiro_bs_base": "master",
        "labboy0276/bootstrap": "master",
        "drupal/ctools": "^3.0",
        "drupal/token": "^1.0",
        "drupal/pathauto": "^1.0",
        "drupal/redirect": "^1.0@beta"
    },
    "replace": {
        "drupal/core": "*"
    }
}
